{
  "manifest_version": 3,
  "name": "API Duplicate Detector",
  "version": "1.0",
  "description": "Detect duplicate API requests in the network.",
  "permissions": ["webRequest", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
